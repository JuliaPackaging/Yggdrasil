 #include <sys/types.h>
 # include <getopt.h>
 # include <inttypes.h>
diff --git a/meson.build b/meson.build
index 6a243a36..6e47fcc1 100644
--- a/meson.build
+++ b/meson.build
@@ -66,7 +66,6 @@ needed_headers = [
   'string.h',
   'unistd.h',
   'stdint.h',
-  'sys/unistd.h',
   'getopt.h',
   'sys/types.h',
   'sys/stat.h',
