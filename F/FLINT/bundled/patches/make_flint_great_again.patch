         result_ui = fmpz_get_ui(result);
         if (n_reverse_table[result_ui] != ctx->qm1)
+        {   /* clean up... */
+            fq_nmod_clear(r, fq_nmod_ctx);
+            fq_nmod_clear(gen, fq_nmod_ctx);
+            flint_free(n_reverse_table);
+            fmpz_clear(result);
+            fmpz_clear(order);
